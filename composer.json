        "ext-zlib": "*",
        "anhskohbo/no-captcha": "^3.2",
        "arcanedev/log-viewer": "^7.0",
        "artesaos/seotools": "^0.18.0",
        "awobaz/compoships": "^2.0",
        "babenkoivan/scout-elasticsearch-driver": "^4.2",
        "bensampo/laravel-enum": "^2.1",
        "coderello/laravel-shared-data": "^2.1",
        "davejamesmiller/laravel-breadcrumbs": "^5.3",
        "doctrine/dbal": "^2.10",
        "emgag/flysystem-hash": "^1.0",
        "fgribreau/mailchecker": "^3.3",
        "fico7489/laravel-pivot": "^3.0",
        "fideloper/proxy": "^4.4",
        "fossil01/laravel-dkim": "dev-master",
        "gabrielelana/byte-units": "^0.5.0",
        "gemorroj/xbbcode": "^2.4",
        "genealabs/laravel-caffeine": "^7.0",
        "genealabs/laravel-model-caching": "^0.10.1",
        "geoip2/geoip2": "^2.10",
        "guzzlehttp/guzzle": "^7.0",
        "hisorange/browser-detect": "^4.2",
        "jenssegers/date": "^4.0",
        "jenssegers/imagehash": "^0.7.0",
        "kalnoy/nestedset": "^5.0",
        "laravel-validation-rules/credit-card": "^1.3",
        "laravel/framework": "^7.0",
        "laravel/socialite": "^4.4",
        "laravel/tinker": "^2.4",
        "laravel/ui": "^2.1",
        "laravelcollective/html": "^6.1",
        "league/flysystem": "^1.1",
        "league/flysystem-memory": "^1.0",
        "league/flysystem-ziparchive": "^1.0",
      "league/html-to-markdown": "^4.10",
        "litlife/book-converter": "dev-dev",
        "litlife/epub": "dev-dev",
        "litlife/fb2": "dev-dev",
        "litlife/fb2-fix": "dev-dev",
        "litlife/fb2-to-html": "dev-dev",
        "litlife/html-splitter": "dev-dev",
        "litlife/id-dirname": "dev-dev",
        "litlife/sitemap": "dev-dev",
        "litlife/unitpay": "dev-dev",
        "litlife/url": "dev-dev",
        "nelexa/zip": "^3.3",
        "paragonie/random_compat": "^9.99",
        "propaganistas/laravel-phone": "^4.2",
        "sabberworm/php-css-parser": "^8.3",
        "simplesoftwareio/simple-qrcode": "^3.0",
        "socialiteproviders/facebook": "^1.0",
        "socialiteproviders/google": "^3.1",
        "socialiteproviders/manager": "^3.5",
        "socialiteproviders/vkontakte": "^4.1",
        "spatie/laravel-activitylog": "^3.14",
        "spatie/url": "^1.3",
        "staudenmeir/eloquent-has-many-deep": "^1.12",
        "stevebauman/purify": "^3.0",
        "sunspikes/clamav-validator": "^2.2",
        "symfony/dom-crawler": "^5.1",
        "torann/geoip": "^1.2",
        "watson/active": "^5.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.4",
        "barryvdh/laravel-ide-helper": "^2.8",
        "beyondcode/laravel-query-detector": "^1.3",
        "facade/ignition": "^2.3",
        "filp/whoops": "^2.7",
        "fzaninotto/faker": "^1.9",
        "laravel/dusk": "^6.4",
        "laravel/homestead": "^11.0",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^4.2",
        "orchestra/testbench": "^5.4",
        "phpunit/phpunit": "^8.5",
        "roquie/laravel-dusk-select2": "^2.0",
        "staudenmeir/dusk-updater": "^1.2",
        "timacdonald/log-fake": "^1.6",
        "nikaia/translation-sheet": "^1.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Library/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
          "Tests\\": "tests/",
          "Litlife\\Epub\\Tests\\": "packages/litlife/epub/tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --reset"
        ]
    },
    "repositories": {
        "0": {
            "type": "path",
            "url": "./packages/litlife/book-converter"
        },
        "1": {
            "type": "path",
            "url": "./packages/litlife/epub"
        },
        "2": {
            "type": "path",
            "url": "./packages/litlife/fb2"
        },
        "3": {
            "type": "path",
            "url": "./packages/litlife/fb2-fix"
        },
        "4": {
            "type": "path",
            "url": "./packages/litlife/fb2-to-html"
        },
        "5": {
            "type": "path",
            "url": "./packages/litlife/html-splitter"
        },
        "6": {
            "type": "path",
            "url": "./packages/litlife/id-dirname"
        },
        "7": {
            "type": "path",
            "url": "./packages/litlife/sitemap"
        },
        "8": {
            "type": "path",
            "url": "./packages/litlife/unitpay"
        },
        "9": {
            "type": "path",
            "url": "./packages/litlife/url"
        }
    }
}
