{
  "name": "litlife/book-converter",
  "description": "Book converter",
  "authors": [
    {
      "name": "Litlife",
      "email": "litlife@yandex.ru"
    }
  ],
  "require": {
    "litlife/url": "*",
    "symfony/process": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "Litlife\\BookConverter\\Facades\\": "src/Facades",
      "Litlife\\BookConverter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Litlife\\BookConverter\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Litlife\\BookConverter\\BookConverterServiceProvider"
      ],
      "aliases": {
        "BookConverter": "Litlife\\BookConverter\\Facades\\BookConverter"
      }
    }
  }
}
